    "graphql": "^16.8.1",
    "next": "15.0.0-rc.0",
    "payload": "3.0.0-beta.43",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
