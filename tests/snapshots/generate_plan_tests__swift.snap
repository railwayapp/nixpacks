---
source: tests/generate_plan_tests.rs
expression: plan
---
{
  "nixpacksVersion": "[version]",
  "buildImage": "[build_image]",
  "variables": {},
  "staticAssets": {},
  "phases": [
    {
      "name": "setup",
      "nixPackages": [
        {
          "name": "coreutils"
        },
        {
          "name": "swift"
        },
        {
          "name": "clang"
        },
        {
          "name": "zlib"
        },
        {
          "name": "zlib.dev"
        }
      ],
      "nixLibraries": [],
      "nixpacksArchive": "[archive]",
      "aptPackages": []
    },
    {
      "name": "install",
      "dependsOn": [
        "setup"
      ],
      "commands": [
        "swift package resolve"
      ],
      "onlyIncludeFiles": [
        "Package.swift"
      ]
    },
    {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "commands": [
        "CC=clang++ swift build -c release --static-swift-stdlib",
        "cp ./.build/release/swift ./swift && rm -rf ./.build"
      ]
    }
  ],
  "startPhase": {
    "cmd": "./swift"
  }
}
