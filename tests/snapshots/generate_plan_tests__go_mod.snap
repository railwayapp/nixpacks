---
source: tests/generate_plan_tests.rs
expression: plan
---
{
  "nixpacksVersion": "[version]",
  "buildImage": "[build_image]",
  "variables": {
    "CGO_ENABLED": "0",
    "NIXPACKS_METADATA": "golang"
  },
  "phases": [
    {
      "name": "install",
      "dependsOn": [
        "setup"
      ],
      "commands": [
        "go mod download"
      ],
      "cacheDirectories": [
        "/root/.cache/go-build"
      ]
    },
    {
      "name": "setup",
      "nixPackages": [
        {
          "name": "go"
        }
      ]
    },
    {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "commands": [
        "go build -o out"
      ],
      "cacheDirectories": [
        "/root/.cache/go-build"
      ]
    }
  ],
  "startPhase": {
    "cmd": "./out",
    "runImage": "debian:bullseye-slim"
  }
}
