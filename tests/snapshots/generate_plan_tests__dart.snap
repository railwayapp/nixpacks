---
source: tests/generate_plan_tests.rs
expression: plan
---
{
  "nixpacksVersion": "[version]",
  "buildImage": "[build_image]",
  "variables": {
    "NIXPACKS_METADATA": "dart"
  },
  "phases": [
    {
      "name": "install",
      "dependsOn": [
        "setup"
      ],
      "commands": [
        "dart pub get"
      ],
      "onlyIncludeFiles": [
        "pubspec.yaml"
      ]
    },
    {
      "name": "setup",
      "nixPackages": [
        {
          "name": "dart"
        }
      ]
    },
    {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "commands": [
        "dart compile exe bin/console_simple.dart"
      ]
    }
  ],
  "startPhase": {
    "cmd": "./bin/console_simple.exe"
  }
}
