{
  "providers": [],
  "buildImage": "[build_image]",
  "variables": {
    "ASPNETCORE_ENVIRONMENT": "Production",
    "ASPNETCORE_URLS": "http://0.0.0.0:3000",
    "DOTNET_ROOT": "/nix/var/nix/profiles/default/",
    "NIXPACKS_METADATA": "c#"
  },
  "phases": {
    "build": {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "cmds": [
        "dotnet publish --no-restore -c Release -o out"
      ]
    },
    "install": {
      "name": "install",
      "dependsOn": [
        "setup"
      ],
      "cmds": [
        "dotnet restore"
      ]
    },
    "setup": {
      "name": "setup",
      "nixPkgs": [
        "dotnet-sdk"
      ],
      "nixOverlays": [],
      "nixpkgsArchive": "[archive]"
    }
  },
  "start": {
    "cmd": "./out/csharp-cli"
  }
}
